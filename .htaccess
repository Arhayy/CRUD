RewriteEngine on

RewriteRule ^beranda\.html$ template.php?module=beranda&id=$1 [L]

RewriteRule ^mahasiswa\.html$ template.php?module=mahasiswa&id=$1 [L]
RewriteRule ^form_mahasiswa\.html$ template.php?module=form_mahasiswa&id=$1 [L]
RewriteRule ^input_mahasiswa\.html$ template.php?module=input_mahasiswa&id=$1 [L]
RewriteRule ^input\.php$ template.php?module=input&id=$1 [L]
RewriteRule ^edit_mahasiswa-(.*)\.html$ template.php?module=edit_mahasiswa&id=$1 [L]
RewriteRule ^update_mahasiswa\.html$ template.php?module=update_mahasiswa&id=$1 [L]
RewriteRule ^hapus_mahasiswa-(.*)\.html$ template.php?module=hapus_mahasiswa&id=$1 [L]

RewriteRule ^tabungan\.html$ template.php?module=tabungan&id=$1 [L]
RewriteRule ^penarikan\.html$ template.php?module=penarikan&id=$1 [L]
RewriteRule ^input_penarikan\.html$ template.php?module=input_penarikan&id=$1 [L]
RewriteRule ^setoran\.html$ template.php?module=setoran&id=$1 [L]
RewriteRule ^input_setoran\.html$ template.php?module=input_setoran&id=$1 [L]
RewriteRule ^detail-(.*)\.html$ template.php?module=detail&id=$1 [L]
Options All -Indexes
